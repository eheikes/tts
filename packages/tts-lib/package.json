{
  "name": "tts-lib",
  "version": "1.0.0-pre.1",
  "description": "Abstraction library for TTS services",
  "keywords": [
    "tts",
    "amazon",
    "google",
    "polly",
    "cloud",
    "aws",
    "text-to-speech"
  ],
  "author": "Eric Heikes <eheikes@gmail.com>",
  "homepage": "https://github.com/eheikes/tts/tree/master/packages/tts-lib#readme",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eheikes/tts.git"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "standard --env jasmine --fix --verbose",
    "report-coverage": "codecov",
    "test": "cross-env JASMINE_CONFIG_PATH=test/jasmine.json nyc jasmine"
  },
  "bugs": {
    "url": "https://github.com/eheikes/tts/issues"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.651.1",
    "@google-cloud/text-to-speech": "^5.0.1",
    "async": "^3.0.0",
    "debug": "^3.1.0",
    "sax": "^1.2.4",
    "sentence-splitter": "^5.0.0",
    "tempfile": "^1.1.1",
    "zen-observable": "^0.10.0"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "cross-env": "^5.2.0",
    "husky": "^9.1.7",
    "jasmine": "^3.5.0",
    "nyc": "^15.0.1",
    "proxyquire": "^2.1.3",
    "standard": "^17.1.0"
  },
  "standard": {
    "env": {
      "node": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/**/*.js",
      "*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "all": false,
    "report-dir": "./coverage"
  }
}
